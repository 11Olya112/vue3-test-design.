{
  "name": "todolist",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "vite build",
    "lint": "vue-cli-service lint",
    "deploy": "gh-pages -d dist",
    "dev": "vite",
    "predeploy": "npm run build",
    "preview": "vite preview"
  },
  "dependencies": {
    "sass": "^1.62.1",
    "vue": "^3.2.47",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.1.3",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/compiler-sfc": "^3.3.2",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.12.0",
    "gh-pages": "^5.0.0",
    "prettier": "^2.4.1",
    "typescript": "^5.0.4",
    "vite": "^4.1.4"
  },
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "jsx": "preserve",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "lib": [
      "esnext",
      "dom"
    ],
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.vue"
  ]
}
